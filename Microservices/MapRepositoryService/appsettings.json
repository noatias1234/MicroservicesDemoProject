{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Settings": {
      "MapRepositoryName": "MapRepository" 
  },
  "Serilog": {
      "Using": [ "Serilog.Sinks.Seq" ],
      "MinimumLevel": {
          "Default": "Information"
      },
      "WriteTo": [
          {
              "Name": "Seq",
              "Args": {
                  "serverUrl": "http://seqLoggerService"
                  //"outputTemplate": "[{Timestamp:yyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId} {Level:u3}] {UserName} {Message:lj}{NewLine}{Exception}"
              }
          }
      ],
      "Properties": {
          "ApplicationName": "MapEntityService"
      }
  }
}
