{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Settings": {
    "MapEntityTopic": "NewMapEntity",
    "GetNewMapEntityClientMethod": "GetNewMapEntity",
    "HostName": "messageBrokerService"
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": { "path": "http://seqLoggerService:5341" }
      }
    ]
  },
  "AllowedHosts": "*"
}